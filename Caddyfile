app.kindplate.ru {
  reverse_proxy frontend:80
  encode gzip zstd
  header {
    Strict-Transport-Security "max-age=31536000; includeSubDomains; preload"
  }
}

api.kindplate.ru {
  reverse_proxy backend:5000
  encode gzip zstd
  header {
    Strict-Transport-Security "max-age=31536000; includeSubDomains; preload"
  }
}


